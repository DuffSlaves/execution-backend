
if [ ! -f ".status/emscripten" ]; then
	# Install emscripten
	git clone https://github.com/duffslaves/emscripten-build.git
	mv emscripten-build emsdk_portable
	chmod +rwxrwxrwx emsdk_portable -R
	cd emsdk_portable
	./emsdk activate latest
	. ./emsdk_env.sh
	cd ../

	echo "" > .status/emscripten
fi
