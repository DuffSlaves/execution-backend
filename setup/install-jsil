
if [ ! -f ".status/JSIL" ]; then
	# Get MSBuild Binaries
	git clone https://github.com/Microsoft/msbuild.git --depth 1 --branch xplat
	cd msbuild
	./init-tools.sh # unix
	cd ../

	# Install JSIL
	git clone https://github.com/sq/JSIL.git --depth 1
	cd JSIL
	git submodule update --init --recursive
	./msbuild/Tools/dotnetcli/dotnet ./Tools/MSBuild.exe JSIL.sln
	cd ../

	echo "" > .status/JSIL
fi